{
  "sourceMap": true,
  "instrument": true,
  "reporter": [
    "html",
    "text"
  ],
  "include": [
    "**/src/**/*.ts"
  ],
  "extension": [
    ".ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.*spec.ts",
    "mocha.config.js",
    "test",
    "**/testing",
    "**/*.fixture.ts",
    "**/local-token.ts"
  ],
  "reportDir": ".coverage"
}
