{
  "compilerOptions": {
    "module": "commonjs",
    "rootDir": ".",
    "outDir": ".out",
    "target": "ES2018",
    "lib": [
      "ES2020"
    ],
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictBindCallApply": true
  }
}
